<h2 class="infowindow_title">
  <% unless object.no_address? %>
    <%= object.address %>
  <% else %>
    <%= object.location %>
  <% end %>
</h2>
<div class="infowindow_details"> 
  <span>racks: <%= object.racks %> / spaces: <%= object.spaces %></span>
  </br>
  <div><%= link_to 'More Information', controller: "bike_parking_spots", action: "show", id: object  %> | <%= link_to "Map It", "https://maps.google.com/maps?z=200&q=loc:#{object.latitude}+#{object.longitude}" %></div>
