!!!
%html
  %head
    %title BikeParkingSanfran
    = stylesheet_link_tag  "application", media: "all", "data-turbolinks-track" => true
    %link{href: "http://fonts.googleapis.com/css?family=Ultra", rel: "stylesheet", type: "text/css"}/
    %link{href: "http://fonts.googleapis.com/css?family=Raleway:500", rel: "stylesheet", type: "text/css"}/
    %script{src: "http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"}
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags
  %body
    = yield
  %footer
    %ul#nav
      %li
        = link_to_unless_current "List", bike_parking_spots_path do |name|
          - "<strong class='nav_link'>#{name}</strong>".html_safe
      %li
        = link_to_unless_current "Map", root_url do |name|
          - "<strong class='nav_link'>#{name}</strong>".html_safe
      %li
        = link_to_unless_current "About Me", "http://www.bobbileehartman.com", target: "_blank"

